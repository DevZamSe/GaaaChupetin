<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorPrimary">#2a4c82</color>
    <color name="colorPrimaryDark">#07a39d</color>
    <color name="colorAccent">#fff</color>
    <color name="ui_components_android_color_primary">@color/colorPrimary</color>

    <!-- Toolbar's text color -->
    <!-- Default: @color/ui_components_white_color -->
    <color name="px_toolbar_text">#fff</color>

    <!-- Status Bar color -->
    <color name="ui_components_android_color_primary_dark">@color/colorPrimary</color>

    <!-- Spinner primary color -->
    <!-- Default: @color/ui_components_android_color_primary -->
    <color name="ui_components_spinner_primary_color">@color/colorPrimary</color>

    <!-- Spinner secondary color -->
    <!-- Default: @color/ui_components_android_color_primary -->
    <color name="ui_components_spinner_secondary_color">@color/colorPrimaryDark</color>

    <!-- Spinner background color -->
    <!-- Default: @color/ui_components_white_color -->
    <color name="px_background_loading">#fff</color>

    <!-- Payment method icon color -->
    <!-- Default: @color/ui_components_android_color_primary -->
    <color name="px_paymentMethodTint">@color/colorPrimary</color>

    <!-- Inputs color -->
    <!-- Default: @color/ui_components_android_color_primary -->
    <color name="px_input">@color/colorPrimary</color>
</resources>



